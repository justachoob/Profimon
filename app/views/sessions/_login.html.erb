<%= form_for(:session, url: login_path) do |f| %>

      <%= f.label :name, "Username: ", class: 'inline_label' %>
      <%= f.text_field :name, class: 'input_field' %>

      <%= f.label :password, "Password: ", class: 'inline_label' %>
      <%= f.password_field :password, class: 'input_field' %>

      <%= f.submit "Log In", class: "form_button" %>
<% end %>

